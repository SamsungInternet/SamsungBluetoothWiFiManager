{
  "name": "SetupThings",
  "description": "Setup your Things Gateway",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export && cp CNAME export/CNAME && touch export/.nojekyll",
    "start": "PORT=8080 node build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "deploy": "git subtree push --prefix export origin gh-pages"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "^0.3.4",
    "sapper": "^0.25.0",
    "serve-static": "^1.13.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "svelte": "^2.8.1",
    "svelte-loader": "^3.1.4",
    "webpack": "^5.75.0"
  }
}
