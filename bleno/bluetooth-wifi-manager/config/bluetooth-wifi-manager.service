[Unit]
 Description=Bluetooth WiFi Manager Service
 After=multi-user.target

[Service]
 Type=idle
 WorkingDirectory=/home/pi/samsung/SamsungBluetoothWiFiManager/bleno/bluetooth-wifi-manager
 ExecStart=/usr/bin/node /home/pi/samsung/SamsungBluetoothWiFiManager/bleno/bluetooth-wifi-manager/bluetooth-wifi-manager.js > /home/pi/samsung/blueto$
 RestartSec=30
 Restart=on-failure
 #StandardOutput=/home/pi/samsung/bluetoothWiFi.log
 #StandardError=/home/pi/samsung/bluetoothWiFi.log


[Install]
 WantedBy=multi-user.target
